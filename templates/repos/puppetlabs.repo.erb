[puppetlabs]
name=puppetlabs
baseurl=<%= @baseurl %>
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-puppetlabs
<% if @proxy %>proxy=<%= @proxy %>
<% end -%>

[puppetlabs-deps]
name=puppetlabs dependencies
baseurl=<%= @baseurl %>
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-puppetlabs
<% if @proxy %>proxy=<%= @proxy %>
<% end -%>
